{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "@vercel/node@3.0.7",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "regions": ["iad1"]
}